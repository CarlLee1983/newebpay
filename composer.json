{
    "name": "carllee1983/newebpay",
    "description": "藍新金流 SDK - NewebPay Payment Gateway SDK for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "newebpay",
        "payment",
        "gateway",
        "taiwan",
        "credit-card",
        "atm",
        "cvs",
        "laravel",
        "sdk"
    ],
    "homepage": "https://github.com/CarlLee1983/newebpay",
    "support": {
        "issues": "https://github.com/CarlLee1983/newebpay/issues",
        "source": "https://github.com/CarlLee1983/newebpay"
    },
    "authors": [
        {
            "name": "Carl Lee",
            "email": "carllee0520@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-openssl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "infection/infection": "^0.31.9",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "CarlLee\\NewebPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarlLee\\NewebPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlLee\\NewebPay\\Laravel\\NewebPayServiceProvider"
            ],
            "aliases": {
                "NewebPay": "CarlLee\\NewebPay\\Laravel\\Facades\\NewebPay"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs": "phpcs --standard=PSR12 src/",
        "cs-fix": "phpcbf --standard=PSR12 src/",
        "analyse": "phpstan analyse --memory-limit=2G",
        "check:security": "composer audit",
        "test:mutation": "infection"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}