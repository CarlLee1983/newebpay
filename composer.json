{
    "name": "carllee1983/newebpay",
    "description": "藍新金流 SDK - NewebPay Payment Gateway SDK for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "newebpay",
        "payment",
        "gateway",
        "taiwan",
        "credit-card",
        "atm",
        "cvs",
        "laravel",
        "sdk"
    ],
    "homepage": "https://github.com/CarlLee1983/newebpay",
    "support": {
        "issues": "https://github.com/CarlLee1983/newebpay/issues",
        "source": "https://github.com/CarlLee1983/newebpay"
    },
    "authors": [
        {
            "name": "Carl Lee",
            "email": "carllee0520@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.4|^1.5|^1.6",
        "orchestra/testbench": "^6.24|^7.0|^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "CarlLee\\NewebPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarlLee\\NewebPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlLee\\NewebPay\\Laravel\\NewebPayServiceProvider"
            ],
            "aliases": {
                "NewebPay": "CarlLee\\NewebPay\\Laravel\\Facades\\NewebPay"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs": "phpcs --standard=PSR12 src/",
        "cs-fix": "phpcbf --standard=PSR12 src/"
    },
    "config": {
        "sort-packages": true,
        "audit": {
            "ignore": {
                "PKSA-8qx3-n5y5-vvnd": "Ignore Laravel 8 security advisory for legacy testbench support"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}